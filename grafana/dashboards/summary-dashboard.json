          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          },
          "editorMode": "code",
          "expr": "count(node_cpu_seconds_total{mode='system'}) by (servername)",
          "format": "table",
          "instant": true,
          "refId": "Cores"
