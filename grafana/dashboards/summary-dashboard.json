          "instant": true,
          "refId": "TotalMemory"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          },
          "expr": "time() - node_boot_time_seconds",
          "format": "table",
          "instant": true,
          "refId": "Uptime"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          },
          "editorMode": "code",
          "expr": "100 * (1 - sum(node_filesystem_free_bytes{fstype!~\"tmpfs|squashfs\"}) by (servername) / sum(node_filesystem_size_bytes{fstype!~\"tmpfs|squashfs\"}) by (servername))",
          "format": "table",
          "instant": true,
          "refId": "Disk"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "PBFA97CFB590B2093"
          },
          "editorMode": "code",
          "expr": "sum(node_filesystem_size_bytes{fstype!~\"tmpfs|squashfs\"}) by (servername)",
          "format": "table",
          "instant": true,
          "refId": "TotalDisk"
        },
        {
